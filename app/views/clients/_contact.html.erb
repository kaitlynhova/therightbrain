
<div id = "contact" class="bottom_nav row">
  <div class="large-6 columns">
    <h1 class="bottom_nav_h1 bottom_header">CLIENT LOG IN</h1>
    <p class="subheader sub2">check in <p>
  </div>
  <div class="large-6 columns">
    <h1 class="bottom_nav_h1 bottom_header">CONTACT </p>
    <p class="subheader">say hello <p>
    <form name="contact" action="/contact" method="post" class="contact_form">    
        <p><%= text_field_tag 'fname', nil, placeholder: 'NAME',:class => "inputtimes" %> </p>
        <p><%= text_area_tag 'description',nil, placeholder: 'MESSAGE',:class => "text-fieldtimes"%></p>
        <p><%= email_field_tag 'email', nil, placeholder: 'EMAIL',:class => "inputtimes" %></p>
      <input type="submit" value="SEND" class="submit_button">  
    </form>
  </div>
</div>

