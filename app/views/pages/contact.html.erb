<h1>CONTACT PAGE</h1>

    <form name="contact" action="/contact" method="post" style="margin-top: 500px;position:relative; z-index: 1000">
     
      <%= text_field_tag 'fname', nil, placeholder: 'NAME' %>
      <%= text_area_tag 'description',nil, placeholder: 'MESSAGE'%>
      <%= email_field_tag 'email', nil, placeholder: 'EMAIL' %>

      <input type="submit" value="GO!" style="background:black;">
  
  
    </form>
